<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zunnorain.noque">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.PatientLoginActivity" />
        <activity android:name=".Activities.SignupActivty" />
        <activity android:name=".Activities.DocPatSelection" />
        <activity android:name=".Activities.DoctorOptions" />
        <activity android:name=".Activities.PatientOptions" />
        <activity android:name=".Activities.MakeAppointment" />
        <activity android:name=".Activities.ViewAppointments" />
        <activity android:name=".Activities.RescheduleAppointment" />
        <activity android:name=".Activities.DoctorsList" />
        <activity android:name=".Activities.DoctorLoginActivity" />
        <activity android:name=".Activities.PatientsList" />
        <activity android:name=".Activities.WritePrescription" />
        <activity android:name=".Activities.ViewPrescription" />
        <activity android:name=".Activities.AboutUs"></activity>
    </application>

</manifest>